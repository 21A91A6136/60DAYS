#include<bits/stdlib.h>
using namespace std;
int mod = 10^9+7;
int power(int a,int b)
{
    int ans = 1;
    while(b)
    {
        if(b&1)
        {
            b = b-1;
            ans*=a
        }
        else
        {
            b = b/2;
            a = a*a;
        }
    }
    return ans;
}
int inverse(int b){
    int x = power(b,mod-2);
}
int main()
{
    int a,b;
    cin>>a>>b;
    cout<<power(a,b);
    cout<<inverse(b);
}
