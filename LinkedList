//Basic
#include<bits/stdc++.h>
using namespace std;
struct Node{
    public:
    int data;
    Node* next;
    public:
    Node(int data1,Node* next1)
    {
        data = data1;
        next = next1;
    }
};
int main(){
    vector<int> arr = {2,3,4,5,6};
    Node* a = new Node(arr[0],nullptr);
    cout<< a->next;
}
//we can use either struct or class
//memory space
for 32bit int uses 4byte and pointer uses 4bytes
for 64bit int uses 4byte and pointer uses 12bytes
